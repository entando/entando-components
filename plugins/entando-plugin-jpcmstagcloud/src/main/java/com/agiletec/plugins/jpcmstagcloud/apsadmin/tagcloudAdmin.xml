<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="core_do/TagcloudAdmin" namespace="/do/TagcloudAdmin" extends="entando-default">

        <action name="configSystemParams" class="baseAdminAction" method="configSystemParams">
            <result type="tiles">admin.jpcmstagcloud.Configuration.edit</result>
        	<interceptor-ref name="entandoDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
        </action>

        <action name="updateSystemParams" class="baseAdminAction" method="updateSystemParams">
            <result type="tiles">admin.jpcmstagcloud.Configuration.edit</result>
        	<interceptor-ref name="entandoDefaultStack"><param name="requestAuth.requiredPermission">superuser</param></interceptor-ref>
        </action>

    </package>

</struts>