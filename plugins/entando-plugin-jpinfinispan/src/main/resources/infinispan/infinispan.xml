		<local-cache name="Entando_ResourceManager">
			<!-- <eviction strategy="NONE" max-entries="1" /> -->
		</local-cache>
		
		<local-cache name="Entando_KeyGeneratorManager">
			<!-- <eviction strategy="NONE" max-entries="1" /> -->
		</local-cache>
	</cache-container>
	
</infinispan>
