		</local-cache>
		<local-cache name="Entando_CategoryManager">
			<!-- <eviction strategy="NONE" max-entries="10000" /> -->
		</local-cache>
		<local-cache name="Entando_ConfigManager">
			<!-- <eviction strategy="NONE" max-entries="100" /> -->
		</local-cache>
		<local-cache name="Entando_RoleManager">
			<!-- <eviction strategy="NONE" max-entries="200" /> -->
