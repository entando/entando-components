		<local-cache name="Entando_DataObjectManager">
		</local-cache>
		<local-cache name="Entando_DataObjectModelManager">
			<!-- <eviction strategy="NONE" max-entries="100" /> -->
		</local-cache>
		<local-cache name="Entando_DataObjectPageMapperManager">
