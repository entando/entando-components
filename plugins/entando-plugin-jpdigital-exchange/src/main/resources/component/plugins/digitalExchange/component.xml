<?xml version="1.0" encoding="UTF-8"?>
<component>
	<code>jpdigital-exchange</code>
	<description>Entando DigitalExchange Plugin</description>
	<dependencies>
		<code>jprestapi</code>
	</dependencies>
	<installation>
		<environment code="production">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/plugins/digitalExchange/port_data_production.sql
				</datasource>
			</defaultSqlResources>
		</environment>
		<environment code="test">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/plugins/digitalExchange/port_data_test.sql
				</datasource>
			</defaultSqlResources>
		</environment>
	</installation>
	<uninstallation>
		<sqlResources>
			<datasource name="portDataSource">
classpath:sql/plugins/digitalExchange/port_uninstallation.sql
			</datasource>
		</sqlResources>
	</uninstallation>
</component>
